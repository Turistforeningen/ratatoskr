{
  "name": "ratatoskr",
  "description": "Ratatoskr",
  "version": "1.0.0-alpha",
  "scripts": {
    "build:prod": "webpack --env.production --env.analyze --optimize-minimize",
    "build:dev": "webpack --env.development --env.analyze --progress --colors",
    "lint": "eslint --ext .js,.jsx frontend/ server/",
    "lint:watch": "esw --color --ext .js,.jsx --watch frontend/ server/",
    "nsp": "nsp check",
    "start": "supervisor -w server -e js server/index.js",
    "test": "mocha -b -c -t 10000 --check-leaks -R tap -r server/test/*.js server/test/**/*.js frontend/test/*.js frontend/test/**/*.js",
    "test:watch": "mocha -w -b -c -t 10000 --check-leaks -R progress -r server/test/*.js server/test/**/*.js frontend/test/*.js frontend/test/**/*.js",
    "webpack:serve": "webpack-dev-server --env.development --color"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "connect-redis": "^3.3.0",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "ioredis": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "nunjucks": "^3.0.0",
    "raven": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "class-to-classname": "^1.0.1",
    "core-decorators": "^0.19.0",
    "css-loader": "^0.28.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^3.4.1",
    "nsp": "^2.6.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "v3.0.0-beta.6",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "supervisor": "^0.12.0",
    "webpack": "^2.6.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^2.4.5"
  }
}
